{
  "expo": {
    "name": "TUNG",
    "slug": "tung-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/TUNGIcon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/TUNGIcon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tung.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "TUNG necesita acceso a tu ubicación para mostrarte eventos de fútbol cerca de ti.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "TUNG necesita acceso a tu ubicación para mostrarte eventos de fútbol cerca de ti.",
        "NSCameraUsageDescription": "TUNG necesita acceso a la cámara para que puedas tomar fotos de perfil y de tus canchas.",
        "NSPhotoLibraryUsageDescription": "TUNG necesita acceso a tu galería para que puedas seleccionar fotos de perfil y de tus canchas.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB3HimoCgt_v_w7bYSKZzGj1uI8C2wefjI"
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#30ce26"
      },
      "package": "com.tung.app",
      "permissions": [
        "android.permission.INTERNET",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.DETECT_SCREEN_CAPTURE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB3HimoCgt_v_w7bYSKZzGj1uI8C2wefjI"
        }
      },
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/images/TUNGIcon.jpg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-location"
    ],
    "scheme": "tung",
    "extra": {
      "eas": {
        "projectId": "cbc62db7-dc73-49eb-b206-d5e251567f46"
      }
    },
    "owner": "reich0319"
  }
}
